#ifndef TEP__SESSION_3_KIT_1E_INC_PICCOM_H_
#define TEP__SESSION_3_KIT_1E_INC_PICCOM_H_

#include "stm32f1xx_hal.h"
#include "stdint.h"
#include "stdbool.h"

#define PIC_FRAME_SIZE 8
#define PIC_TRAME_START 'G'
#define PIC_TRAME_CONFIRM 'O'

typedef struct {
	uint8_t x;
	uint8_t y;
	uint8_t pince;
	uint8_t balance;
} PIC_Status;



